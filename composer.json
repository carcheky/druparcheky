{
    "name": "carcheky/druparcheky_core_composer_project",
    "description": "Project template for Druparcheky",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [{
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "carcheky/druparcheky_theme",
                "version": "dev-master",
                "type": "drupal-theme",
                "source": {
                    "url": "https://github.com/carcheky/druparcheky_theme.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "carcheky/druparcheky_profile",
                "version": "dev-master",
                "type": "drupal-profile",
                "source": {
                    "url": "https://github.com/carcheky/druparcheky_profile.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }

    ],
    "require": {
        "carcheky/druparcheky_profile": "dev-master",
        "carcheky/druparcheky_theme": "dev-master",
        "composer/installers": "1.7.0",
        "drupal/admin_toolbar": "^2.2",
        "drupal/backup_migrate": "^4.1",
        "drupal/config_devel": "^1.4",
        "drupal/core-composer-scaffold": "^8.8.4",
        "drupal/core-recommended": "^8.8.4",
        "drupal/ctools": "^3.2",
        "drupal/devel": "^2.1",
        "drupal/imce": "^1.7",
        "drupal/layout_section_classes": "^1.0@RC",
        "drupal/module_filter": "^3.1",
        "drupal/pathauto": "^1.6",
        "drupal/token": "^1.6",
        "drush/drush": "^8.3"
    },
    "require-dev": {},
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "pre-install-cmd": [
            "bash ./scripts/composer-pre-install"
        ],
        "pre-update-cmd": [
            "bash ./scripts/composer-pre-install"
        ],
        "post-install-cmd": [
            "bash ./scripts/composer-post-install"
        ],
        "post-update-cmd": [
            "bash ./scripts/composer-post-install"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    }
}
